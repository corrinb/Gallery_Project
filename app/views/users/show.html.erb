<div class="row">
<div class="small-10 columns">
  <h1><%= @user.username %></h1>
</div>
</div>

<div class="row">
<div class="small-12 columns">
  <ul class="small-block-grid-5">
  <% @user.images.each do |image| %>
    <li><%= link_to (image_tag(image.submission.thumb, size: "200x200")), image_path(image) %></li>
  <% end %>
  </ul>
</div>
</div>
